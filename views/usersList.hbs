    <div class="container" style="margin-top: 6%">
    <div class="row">
        <div class="col-md-8">
          <form action="/user/searchUsers" method="post">
            <div class="input-group">
              <div class="form-outline" data-mdb-input-init>
                <input type="search" id="form1" class="form-control" name="searchTerm" placeholder="Search" value="{{ searchTerm }}">
              </div>
              <button type="submit  " class="btn btn-primary" style="height: fit-content;" data-mdb-ripple-init>
              <i class="fa fa-search " aria-hidden="true"></i>
              </button>
            </div>
            </form>

            {{!-- Display the list of users --}}
            {{#if users.length}}
               <ul>
            {{#each users}}
               <li>{{username}}</li>
            {{/each}}
               </ul>
            {{else}}
               <p>No users found</p>
            {{/if}}


